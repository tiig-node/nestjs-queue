{
  "name": "@npkgdev/nestjs-queue",
  "version": "1.0.1",
  "description": "Queue manager for NestJS Framework",
  "author": "Douglas de Paula Natalli",
  "license": "MIT",
  "url": "https://github.com/npkgdev/nestjs-queue#readme",
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig.json",
    "format": "prettier --write \"lib/**/*.ts\"",
    "lint": "eslint \"lib/**/*.ts\" --fix",
    "prepublish:npm": "npm run build",
    "publish:npm": "npm publish --access public",
    "prepublish:next": "npm run build",
    "publish:next": "npm publish --access public --tag next",
    "test:e2e": "jest --config ./tests/jest-e2e.json --runInBand",
    "test:e2e:dev": "jest --config ./tests/jest-e2e.json --runInBand --watch",
    "prepare": "husky install",
    "prerelease": "npm run build",
    "release": "npx semantic-release"
  },
  "dependencies": {
    "better-queue": "^3.8.10"
  },
  "devDependencies": {
    "@commitlint/cli": "17.6.5",
    "@commitlint/config-angular": "17.6.5",
    "@nestjs/common": "8.4.7",
    "@nestjs/core": "9.4.2",
    "@nestjs/testing": "8.4.7",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/commit-analyzer": "^9.0.2",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "8.1.0",
    "@semantic-release/npm": "^9.0.0",
    "@semantic-release/release-notes-generator": "^10.0.3",
    "@types/better-queue": "^3.8.3",
    "@types/jest": "29.5.2",
    "@types/node": "18.16.17",
    "@typescript-eslint/eslint-plugin": "5.59.9",
    "@typescript-eslint/parser": "5.59.9",
    "eslint": "8.42.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-import": "2.27.5",
    "husky": "8.0.3",
    "jest": "29.5.0",
    "lint-staged": "13.2.2",
    "prettier": "2.8.8",
    "reflect-metadata": "0.1.13",
    "rimraf": "5.0.1",
    "semantic-release": "19.0.5",
    "ts-jest": "29.1.0",
    "typescript": "5.1.3"
  },
  "peerDependencies": {
    "@nestjs/common": "^8.0.0",
    "@nestjs/core": "^8.0.0 || ^9.0.0"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/npkgdev/nestjs-queue"
  },
  "engine": {
    "node": ">=12"
  }
}
